{
  "$defs": {
    "After": {
      "properties": {
        "success": {
          "description": "成功时的任务名列表, 任务完成后逐个执行",
          "items": {
            "type": "string"
          },
          "title": "Success",
          "type": "array"
        },
        "failure": {
          "description": "失败时的任务名列表, 任务完成后逐个执行",
          "items": {
            "type": "string"
          },
          "title": "Failure",
          "type": "array"
        },
        "always": {
          "description": "无论成功或失败都执行的任务名列表, 任务完成后逐个执行",
          "items": {
            "type": "string"
          },
          "title": "Always",
          "type": "array"
        },
        "ignore_errors": {
          "default": false,
          "description": "是否忽略错误",
          "title": "Ignore Errors",
          "type": "boolean"
        }
      },
      "title": "After",
      "type": "object"
    },
    "Before": {
      "properties": {
        "tasks": {
          "description": "使用的任务名列表, 任务开始前逐个执行",
          "items": {
            "type": "string"
          },
          "title": "Tasks",
          "type": "array"
        },
        "ignore_errors": {
          "default": false,
          "description": "是否忽略错误",
          "title": "Ignore Errors",
          "type": "boolean"
        }
      },
      "title": "Before",
      "type": "object"
    },
    "Calculate": {
      "properties": {
        "expressions": {
          "additionalProperties": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "number"
              },
              {
                "type": "integer"
              }
            ]
          },
          "description": "计算表达式字典, 键为变量名, 值为表达式字符串或直接的数值, 表达式可以包含其他变量名变量直接用字母组合表示, 例如: {'a': 'b * b - c', 'b': 'a ** 0.5 + 1'}可用表达式: 加法'+', 减法'-', 乘法'*', 真除'/', 次方'**', 整除'//', 取模'%'函数调用: 以$开头, 例如: {'c': '$sqrt(a)'}",
          "title": "Expressions",
          "type": "object"
        },
        "variables": {
          "additionalProperties": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "number"
              }
            ]
          },
          "description": "计算中使用的变量及其值",
          "title": "Variables",
          "type": "object"
        },
        "returns": {
          "additionalProperties": {
            "type": "string"
          },
          "description": "计算结果返回的变量名, 键为新变量名, 值为表达式变量名",
          "title": "Returns",
          "type": "object"
        }
      },
      "required": [
        "expressions"
      ],
      "title": "Calculate",
      "type": "object"
    },
    "Delay": {
      "properties": {
        "pre": {
          "default": 0,
          "description": "前置延时(ms), 任务执行前等待时间",
          "minimum": 0,
          "title": "Pre",
          "type": "integer"
        },
        "post": {
          "default": 0,
          "description": "后置延时(ms), 任务执行后等待时间",
          "minimum": 0,
          "title": "Post",
          "type": "integer"
        }
      },
      "title": "Delay",
      "type": "object"
    },
    "Globals": {
      "additionalProperties": true,
      "properties": {
        "debug": {
          "default": false,
          "description": "调试模式, 开启后输出详细日志",
          "title": "Debug",
          "type": "boolean"
        },
        "colorful": {
          "default": true,
          "description": "彩色日志输出",
          "title": "Colorful",
          "type": "boolean"
        },
        "ignore": {
          "default": false,
          "description": "忽略错误",
          "title": "Ignore",
          "type": "boolean"
        },
        "logConfig": {
          "$ref": "#/$defs/LogConfig",
          "description": "日志配置"
        }
      },
      "title": "Globals",
      "type": "object"
    },
    "Input": {
      "properties": {
        "type": {
          "description": "输入类型, 可选: Mouse(鼠标输入), Keyboard(键盘输入), Text(文本输入)",
          "enum": [
            "Mouse",
            "Keyboard",
            "Text"
          ],
          "title": "Type",
          "type": "string"
        },
        "mouse": {
          "anyOf": [
            {
              "$ref": "#/$defs/Input_Mouse"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "鼠标输入定义, 仅在type为Mouse时有效"
        },
        "keyboard": {
          "anyOf": [
            {
              "$ref": "#/$defs/Input_Keyboard"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "键盘输入定义, 仅在type为Keyboard时有效"
        },
        "text": {
          "anyOf": [
            {
              "$ref": "#/$defs/Input_Text"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "文本输入定义, 仅在type为Text时有效"
        },
        "background": {
          "default": false,
          "description": "是否在后台执行, 默认为False, 如果为True, 则尝试后台输入",
          "title": "Background",
          "type": "boolean"
        },
        "focus": {
          "default": false,
          "description": "是否在执行输入前将窗口置于前台, 默认为False, 如果为True, 则不改变当前焦点窗口",
          "title": "Focus",
          "type": "boolean"
        },
        "title": {
          "default": "",
          "description": "窗口标题, 仅在后台操作时有效",
          "title": "Title",
          "type": "string"
        },
        "class_name": {
          "default": "",
          "description": "窗口类名, 仅在后台操作时有效",
          "title": "Class Name",
          "type": "string"
        },
        "process": {
          "default": "",
          "description": "进程名称, 仅在后台操作时有效",
          "title": "Process",
          "type": "string"
        },
        "visible_only": {
          "default": true,
          "description": "是否只查找可见窗口, 默认为True, 如果为False, 则查找所有窗口",
          "title": "Visible Only",
          "type": "boolean"
        },
        "exact_match": {
          "default": false,
          "description": "是否精确匹配窗口, 默认为False, 如果为True, 则只查找完全匹配的窗口",
          "title": "Exact Match",
          "type": "boolean"
        }
      },
      "required": [
        "type"
      ],
      "title": "Input",
      "type": "object"
    },
    "Input_Keyboard": {
      "properties": {
        "type": {
          "description": "键盘输入类型, 可选: Press(按下), Release(释放), Type(输入)",
          "enum": [
            "Press",
            "Release",
            "Type"
          ],
          "title": "Type",
          "type": "string"
        },
        "keys": {
          "description": "键盘输入的按键列表, 如['ctrl', 'v'], 支持组合键",
          "items": {
            "type": "string"
          },
          "title": "Keys",
          "type": "array"
        },
        "duration": {
          "default": 0,
          "description": "延时(ms), 完全按下到释放的时间",
          "minimum": 0,
          "title": "Duration",
          "type": "integer"
        },
        "sep_time": {
          "default": 0,
          "description": "按键间隔时间(ms), 用于按键间的延时",
          "minimum": 0,
          "title": "Sep Time",
          "type": "integer"
        },
        "returns": {
          "additionalProperties": {
            "enum": [
              "keys",
              "type",
              "duration"
            ],
            "type": "string"
          },
          "description": "返回值变量字典, 包含['keys', 'type', 'duration'],以键为变量, 值指定返回参数, 可在其他Job中使用use指定该job返回的参数",
          "title": "Returns",
          "type": "object"
        }
      },
      "required": [
        "type"
      ],
      "title": "Input_Keyboard",
      "type": "object"
    },
    "Input_Mouse": {
      "properties": {
        "type": {
          "description": "鼠标输入类型, 可选: Press(按下), Release(释放), Click(点击), Move(移动),  Drag(拖动)",
          "enum": [
            "Press",
            "Release",
            "Click",
            "Move",
            "Drag"
          ],
          "title": "Type",
          "type": "string"
        },
        "button": {
          "default": "LEFT",
          "description": "鼠标按钮类型, 可选: LEFT(左键), RIGHT(右键), MIDDLE(中键)",
          "enum": [
            "LEFT",
            "RIGHT",
            "MIDDLE"
          ],
          "title": "Button",
          "type": "string"
        },
        "x": {
          "default": 0,
          "description": "X坐标, 像素值",
          "title": "X",
          "type": "integer"
        },
        "y": {
          "default": 0,
          "description": "Y坐标, 像素值",
          "title": "Y",
          "type": "integer"
        },
        "duration": {
          "default": 0,
          "description": "延时(ms), 鼠标操作时间",
          "minimum": 0,
          "title": "Duration",
          "type": "integer"
        },
        "relative": {
          "default": true,
          "description": "是否相对当前位置移动, 默认为True, 如果为True, 则x和y表示相对偏移量",
          "title": "Relative",
          "type": "boolean"
        },
        "returns": {
          "additionalProperties": {
            "enum": [
              "origin_x",
              "origin_y",
              "x",
              "y",
              "duration",
              "button",
              "type"
            ],
            "type": "string"
          },
          "description": "返回值变量字典, 包含['origin_x', 'origin_y', 'x', 'y', 'duration', 'button', 'type'], 以键为变量, 值指定返回参数, 可在其他Job中使用use指定该job返回的参数",
          "title": "Returns",
          "type": "object"
        }
      },
      "required": [
        "type"
      ],
      "title": "Input_Mouse",
      "type": "object"
    },
    "Input_Text": {
      "properties": {
        "message": {
          "description": "要输入的文本内容",
          "title": "Message",
          "type": "string"
        },
        "duration": {
          "default": 0,
          "description": "延时(ms), 文本输入时间",
          "minimum": 0,
          "title": "Duration",
          "type": "integer"
        },
        "returns": {
          "additionalProperties": {
            "enum": [
              "type",
              "message",
              "duration"
            ],
            "type": "string"
          },
          "description": "返回值变量字典, 包含['type', 'message', 'duration']以键为变量, 值指定返回参数, 可在其他Job中使用use指定该job返回的参数",
          "title": "Returns",
          "type": "object"
        }
      },
      "required": [
        "message"
      ],
      "title": "Input_Text",
      "type": "object"
    },
    "Job": {
      "properties": {
        "name": {
          "default": "",
          "description": "任务名称，仅用于编程记录，不会出现在 schema 中",
          "title": "Name",
          "type": "string"
        },
        "type": {
          "description": "任务类型, 可选: ROI(区域识别), OCR(文字识别), Input(输入操作), System(系统操作), Overload(继承), Calculate(计算)",
          "enum": [
            "ROI",
            "OCR",
            "Input",
            "System",
            "Overload",
            "Calculate"
          ],
          "title": "Type",
          "type": "string"
        },
        "roi": {
          "anyOf": [
            {
              "$ref": "#/$defs/ROI"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "ROI任务定义, 仅在type为ROI时有效, 包含图像和区域信息"
        },
        "input": {
          "anyOf": [
            {
              "$ref": "#/$defs/Input"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "输入操作定义, 仅在type为Input时有效, 包含鼠标/键盘/文本输入"
        },
        "system": {
          "anyOf": [
            {
              "$ref": "#/$defs/System"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "系统操作定义, 仅在type为System时有效"
        },
        "calculate": {
          "anyOf": [
            {
              "$ref": "#/$defs/Calculate"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "计算任务定义, 仅在type为Calculate时有效"
        },
        "overload": {
          "default": "",
          "description": "继承的任务名, 仅在type为Overload时有效, 允许继承其他任务的定义",
          "title": "Overload",
          "type": "string"
        },
        "description": {
          "default": "",
          "description": "任务描述, 便于理解用途",
          "title": "Description",
          "type": "string"
        },
        "before": {
          "$ref": "#/$defs/Before",
          "description": "使用的前置任务名列表, 任务开始前逐个执行"
        },
        "after": {
          "$ref": "#/$defs/After",
          "description": "使用的任务名列表, 任务完成后逐个执行"
        },
        "next": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "$ref": "#/$defs/Next"
            }
          ],
          "default": "",
          "description": "下一个任务名或分支, 支持 success/failure 分支",
          "title": "Next"
        },
        "delay": {
          "$ref": "#/$defs/Delay",
          "description": "延时设置, 控制任务前中后等待时间"
        },
        "limits": {
          "$ref": "#/$defs/Limits",
          "description": "任务执行限制, 包含最大执行次数、失败次数等"
        },
        "needs": {
          "description": "依赖的前置任务名, 只有全部完成后才会执行本任务",
          "items": {
            "type": "string"
          },
          "title": "Needs",
          "type": "array"
        },
        "use": {
          "default": "",
          "description": "指定一个Job, 可使用=该job返回的参数",
          "title": "Use",
          "type": "string"
        }
      },
      "required": [
        "type"
      ],
      "title": "Job",
      "type": "object"
    },
    "Limits": {
      "properties": {
        "maxCount": {
          "default": -1,
          "description": "最大执行次数, 默认不限制",
          "minimum": -1,
          "title": "Maxcount",
          "type": "integer"
        },
        "maxFailure": {
          "default": -1,
          "description": "最大失败次数, 默认不限制",
          "minimum": -1,
          "title": "Maxfailure",
          "type": "integer"
        },
        "maxSuccess": {
          "default": -1,
          "description": "最大成功次数, 默认不限制",
          "minimum": -1,
          "title": "Maxsuccess",
          "type": "integer"
        },
        "exit": {
          "default": "",
          "description": "达到最大次数后退出的任务名, 如果为空(null)则退出, 否则切换到指定任务",
          "title": "Exit",
          "type": "string"
        }
      },
      "title": "Limits",
      "type": "object"
    },
    "LogConfig": {
      "properties": {
        "level": {
          "default": "LOG",
          "description": "日志级别, 可选: LOG, DEBUG, INFO, WARNING, ERROR, CRITICAL",
          "enum": [
            "LOG",
            "DEBUG",
            "INFO",
            "WARNING",
            "ERROR",
            "CRITICAL"
          ],
          "title": "Level",
          "type": "string"
        },
        "file": {
          "default": "",
          "description": "日志文件路径, 如果为None则不写入文件",
          "title": "File",
          "type": "string"
        },
        "format": {
          "default": "%(levelname)s - %(asctime)s - %(message)s",
          "description": "日志格式化字符串",
          "title": "Format",
          "type": "string"
        },
        "datefmt": {
          "default": "%Y-%m-%d %H:%M:%S.%f",
          "description": "日期时间格式化字符串",
          "title": "Datefmt",
          "type": "string"
        },
        "clear": {
          "default": false,
          "description": "是否清空日志",
          "title": "Clear",
          "type": "boolean"
        }
      },
      "title": "LogConfig",
      "type": "object"
    },
    "Next": {
      "properties": {
        "success": {
          "default": "",
          "description": "成功时的下一个任务名",
          "title": "Success",
          "type": "string"
        },
        "failure": {
          "default": "",
          "description": "失败时的下一个任务名",
          "title": "Failure",
          "type": "string"
        }
      },
      "title": "Next",
      "type": "object"
    },
    "ROI": {
      "properties": {
        "type": {
          "description": "ROI检测后操作类型, 可选: MoveMouse(移动鼠标), DetectOnly(仅检测)",
          "enum": [
            "MoveMouse",
            "DetectOnly"
          ],
          "title": "Type",
          "type": "string"
        },
        "image": {
          "$ref": "#/$defs/ROI_Image",
          "description": "ROI图像定义, 包含路径和置信度"
        },
        "window": {
          "anyOf": [
            {
              "$ref": "#/$defs/ROI_Window"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "ROI窗口定义, 包含标题, 类名和进程名称, 注意: 某些窗口可能反截屏, 而且可能存在截图兼容性问题"
        },
        "region": {
          "anyOf": [
            {
              "$ref": "#/$defs/ROI_Region"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "ROI区域定义, 包含x, y, width, height"
        },
        "duration": {
          "default": 0,
          "description": "鼠标移动时间, 单位为毫秒",
          "title": "Duration",
          "type": "integer"
        },
        "debug": {
          "$ref": "#/$defs/ROI_Debug",
          "description": "调试参数"
        },
        "returns": {
          "additionalProperties": {
            "enum": [
              "center_x",
              "center_y",
              "confidence",
              "left",
              "top",
              "right",
              "bottom",
              "width",
              "height",
              "template_height",
              "template_width"
            ],
            "type": "string"
          },
          "description": "返回值变量字典, 包含['center_x', 'center_y', 'confidence', 'left', 'top', 'right', 'bottom', 'width', 'height', 'template_height', 'template_width'], 以键为变量, 值指定返回参数, 可在其他Job中使用use指定该job返回的参数",
          "title": "Returns",
          "type": "object"
        }
      },
      "required": [
        "type",
        "image"
      ],
      "title": "ROI",
      "type": "object"
    },
    "ROI_Debug": {
      "properties": {
        "display_screenshot": {
          "default": false,
          "description": "是否显示截图, 默认为False",
          "title": "Display Screenshot",
          "type": "boolean"
        }
      },
      "title": "ROI_Debug",
      "type": "object"
    },
    "ROI_Image": {
      "properties": {
        "path": {
          "description": "图像文件路径",
          "title": "Path",
          "type": "string"
        },
        "confidence": {
          "description": "图像识别置信度",
          "title": "Confidence",
          "type": "number"
        }
      },
      "required": [
        "path",
        "confidence"
      ],
      "title": "ROI_Image",
      "type": "object"
    },
    "ROI_Region": {
      "properties": {
        "x": {
          "description": "识别区域的X坐标",
          "title": "X",
          "type": "integer"
        },
        "y": {
          "description": "识别区域的Y坐标",
          "title": "Y",
          "type": "integer"
        },
        "width": {
          "description": "识别区域的宽度",
          "title": "Width",
          "type": "integer"
        },
        "height": {
          "description": "识别区域的高度",
          "title": "Height",
          "type": "integer"
        }
      },
      "required": [
        "x",
        "y",
        "width",
        "height"
      ],
      "title": "ROI_Region",
      "type": "object"
    },
    "ROI_Window": {
      "properties": {
        "title": {
          "default": "",
          "description": "窗口标题, 可选",
          "title": "Title",
          "type": "string"
        },
        "class_name": {
          "default": "",
          "description": "窗口类名, 可选",
          "title": "Class Name",
          "type": "string"
        },
        "process": {
          "default": "",
          "description": "进程名称, 可选",
          "title": "Process",
          "type": "string"
        },
        "allow_overlay": {
          "default": true,
          "description": "是否允许其他窗口覆盖, 可选",
          "title": "Allow Overlay",
          "type": "boolean"
        },
        "allow_out_of_screen": {
          "default": false,
          "description": "是否允许截屏区域超出屏幕, 可选",
          "title": "Allow Out Of Screen",
          "type": "boolean"
        }
      },
      "title": "ROI_Window",
      "type": "object"
    },
    "System": {
