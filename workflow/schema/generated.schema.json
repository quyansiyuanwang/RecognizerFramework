{
  "$defs": {
    "After": {
      "properties": {
        "success": {
          "default": [],
          "description": "成功时的任务名列表, 任务完成后逐个执行",
          "items": {
            "type": "string"
          },
          "title": "Success",
          "type": "array"
        },
        "failure": {
          "default": [],
          "description": "失败时的任务名列表, 任务完成后逐个执行",
          "items": {
            "type": "string"
          },
          "title": "Failure",
          "type": "array"
        },
        "always": {
          "default": [],
          "description": "无论成功或失败都执行的任务名列表, 任务完成后逐个执行",
          "items": {
            "type": "string"
          },
          "title": "Always",
          "type": "array"
        },
        "ignore_errors": {
          "default": false,
          "description": "是否忽略错误",
          "title": "Ignore Errors",
          "type": "boolean"
        }
      },
      "title": "After",
      "type": "object"
    },
    "Before": {
      "properties": {
        "tasks": {
          "default": [],
          "description": "使用的任务名列表, 任务开始前逐个执行",
          "items": {
            "type": "string"
          },
          "title": "Tasks",
          "type": "array"
        },
        "ignore_errors": {
          "default": false,
          "description": "是否忽略错误",
          "title": "Ignore Errors",
          "type": "boolean"
        }
      },
      "title": "Before",
      "type": "object"
    },
    "Delay": {
      "properties": {
        "pre": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": 0,
          "description": "前置延时(ms), 任务执行前等待时间",
          "title": "Pre"
        },
        "post": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": 0,
          "description": "后置延时(ms), 任务执行后等待时间",
          "title": "Post"
        }
      },
      "title": "Delay",
      "type": "object"
    },
    "IdentifiedGlobals": {
      "properties": {
        "debug": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": false,
          "description": "调试模式, 开启后输出详细日志",
          "title": "Debug"
        },
        "colorful": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": true,
          "description": "彩色日志输出",
          "title": "Colorful"
        },
        "ignore": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": false,
          "description": "忽略错误",
          "title": "Ignore"
        },
        "logConfig": {
          "anyOf": [
            {
              "$ref": "#/$defs/LogConfig"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "日志配置"
        }
      },
      "title": "IdentifiedGlobals",
      "type": "object"
    },
    "Image": {
      "properties": {
        "path": {
          "description": "图像文件路径",
          "title": "Path",
          "type": "string"
        },
        "confidence": {
          "description": "图像识别置信度",
          "title": "Confidence",
          "type": "number"
        }
      },
      "required": [
        "path",
        "confidence"
      ],
      "title": "Image",
      "type": "object"
    },
    "Input": {
      "properties": {
        "type": {
          "description": "输入类型, 可选: Mouse(鼠标输入), Keyboard(键盘输入), Text(文本输入)",
          "enum": [
            "Mouse",
            "Keyboard",
            "Text"
          ],
          "title": "Type",
          "type": "string"
        },
        "mouse": {
          "anyOf": [
            {
              "$ref": "#/$defs/Input_Mouse"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "鼠标输入定义, 仅在type为Mouse时有效"
        },
        "keyboard": {
          "anyOf": [
            {
              "$ref": "#/$defs/Input_Keyboard"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "键盘输入定义, 仅在type为Keyboard时有效"
        },
        "text": {
          "anyOf": [
            {
              "$ref": "#/$defs/Input_Text"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "文本输入定义, 仅在type为Text时有效"
        }
      },
      "required": [
        "type"
      ],
      "title": "Input",
      "type": "object"
    },
    "Input_Keyboard": {
      "properties": {
        "type": {
          "description": "键盘输入类型, 可选: Press(按下), Release(释放), Type(输入)",
          "enum": [
            "Press",
            "Release",
            "Type"
          ],
          "title": "Type",
          "type": "string"
        },
        "keys": {
          "description": "键盘输入的按键列表, 如['ctrl', 'v'], 支持组合键",
          "items": {
            "type": "string"
          },
          "title": "Keys",
          "type": "array"
        },
        "duration": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": 0,
          "description": "延时(ms), 完全按下到释放的时间",
          "title": "Duration"
        },
        "sep_time": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": 0,
          "description": "按键间隔时间(ms), 用于按键间的延时",
          "title": "Sep Time"
        },
        "returns": {
          "anyOf": [
            {
              "additionalProperties": {
                "enum": [
                  "keys",
                  "type",
                  "duration"
                ],
                "type": "string"
              },
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "返回值变量字典, 包含['keys', 'type', 'duration'],以键为变量, 值指定返回参数, 可在其他Job中使用use指定该job返回的参数",
          "title": "Returns"
        }
      },
      "required": [
        "type",
        "keys"
      ],
      "title": "Input_Keyboard",
      "type": "object"
    },
    "Input_Mouse": {
      "properties": {
        "type": {
          "description": "鼠标输入类型, 可选: Press(按下), Release(释放), Click(点击), Move(移动),  Drag(拖动)",
          "enum": [
            "Press",
            "Release",
            "Click",
            "Move",
            "Drag"
          ],
          "title": "Type",
          "type": "string"
        },
        "button": {
          "anyOf": [
            {
              "enum": [
                "LEFT",
                "RIGHT",
                "MIDDLE"
              ],
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "鼠标按钮类型, 可选: LEFT(左键), RIGHT(右键), MIDDLE(中键)",
          "title": "Button"
        },
        "x": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "X坐标, 像素值",
          "title": "X"
        },
        "y": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Y坐标, 像素值",
          "title": "Y"
        },
        "duration": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": 0,
          "description": "延时(ms), 鼠标操作时间",
          "title": "Duration"
        },
        "relative": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": false,
          "description": "是否相对当前位置移动, 默认为False, 如果为True, 则x和y表示相对偏移量",
          "title": "Relative"
        },
        "returns": {
          "anyOf": [
            {
              "additionalProperties": {
                "enum": [
                  "origin_x",
                  "origin_y",
                  "x",
                  "y",
                  "duration",
                  "button",
                  "type"
                ],
                "type": "string"
              },
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "返回值变量字典, 包含['origin_x', 'origin_y', 'x', 'y', 'duration', 'button', 'type'], 以键为变量, 值指定返回参数, 可在其他Job中使用use指定该job返回的参数",
          "title": "Returns"
        }
      },
      "required": [
        "type"
      ],
      "title": "Input_Mouse",
      "type": "object"
    },
    "Input_Text": {
      "properties": {
        "message": {
          "description": "要输入的文本内容",
          "title": "Message",
          "type": "string"
        },
        "duration": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": 0,
          "description": "延时(ms), 文本输入时间",
          "title": "Duration"
        },
        "returns": {
          "anyOf": [
            {
              "additionalProperties": {
                "enum": [
                  "type",
                  "message",
                  "duration"
                ],
                "type": "string"
              },
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "返回值变量字典, 包含['type', 'message', 'duration']以键为变量, 值指定返回参数, 可在其他Job中使用use指定该job返回的参数",
          "title": "Returns"
        }
      },
      "required": [
        "message"
      ],
      "title": "Input_Text",
      "type": "object"
    },
    "Job": {
      "properties": {
        "type": {
          "description": "任务类型, 可选: ROI(区域识别), OCR(文字识别), Input(输入操作), System(系统操作), Overload(继承)",
          "enum": [
            "ROI",
            "OCR",
            "Input",
            "System",
            "Overload"
          ],
          "title": "Type",
          "type": "string"
        },
        "roi": {
          "anyOf": [
            {
              "$ref": "#/$defs/ROI"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "ROI任务定义, 仅在type为ROI时有效, 包含图像和区域信息"
        },
        "input": {
          "anyOf": [
            {
              "$ref": "#/$defs/Input"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "输入操作定义, 仅在type为Input时有效, 包含鼠标/键盘/文本输入"
        },
        "system": {
          "anyOf": [
            {
              "$ref": "#/$defs/System"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "系统操作定义, 仅在type为System时有效"
        },
        "overload": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "继承的任务名, 仅在type为Overload时有效, 允许继承其他任务的定义",
          "title": "Overload"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": "",
          "description": "任务描述, 便于理解用途",
          "title": "Description"
        },
        "before": {
          "anyOf": [
            {
              "$ref": "#/$defs/Before"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "使用的前置任务名列表, 任务开始前逐个执行"
        },
        "after": {
          "anyOf": [
            {
              "$ref": "#/$defs/After"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "使用的任务名列表, 任务完成后逐个执行"
        },
        "next": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "$ref": "#/$defs/Next"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "下一个任务名或分支, 支持 success/failure 分支",
          "title": "Next"
        },
        "delay": {
          "anyOf": [
            {
              "$ref": "#/$defs/Delay"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "延时设置, 控制任务前中后等待时间"
        },
        "limits": {
          "anyOf": [
            {
              "$ref": "#/$defs/Limits"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "任务执行限制, 包含最大执行次数、失败次数等"
        },
        "needs": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": [],
          "description": "依赖的前置任务名, 只有全部完成后才会执行本任务",
          "title": "Needs"
        },
        "use": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "指定一个Job, 可使用=该job返回的参数",
          "title": "Use"
        }
      },
      "required": [
        "type"
      ],
      "title": "Job",
      "type": "object"
    },
    "Limits": {
      "properties": {
        "maxCount": {
          "anyOf": [
            {
              "minimum": 1,
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": -1,
          "description": "最大执行次数, 默认不限制",
          "title": "Maxcount"
        },
        "maxFailure": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": -1,
          "description": "最大失败次数, 默认不限制",
          "title": "Maxfailure"
        },
        "maxSuccess": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": -1,
          "description": "最大成功次数, 默认不限制",
          "title": "Maxsuccess"
        },
        "exit": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "达到最大次数后退出的任务名, 如果为空(null)则退出, 否则切换到指定任务",
          "title": "Exit"
        }
      },
      "title": "Limits",
      "type": "object"
    },
    "LogConfig": {
      "properties": {
        "level": {
          "anyOf": [
            {
              "enum": [
                "LOG",
                "DEBUG",
                "INFO",
                "WARNING",
                "ERROR",
                "CRITICAL"
              ],
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "日志级别, 可选: LOG, DEBUG, INFO, WARNING, ERROR, CRITICAL",
          "title": "Level"
        },
        "file": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": "",
          "description": "日志文件路径, 如果为None则不写入文件",
          "title": "File"
        },
        "format": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": "%(levelname)s - %(asctime)s - %(message)s",
          "description": "日志格式化字符串",
          "title": "Format"
        },
        "datefmt": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": "%Y-%m-%d %H:%M:%S.%f",
          "description": "日期时间格式化字符串",
          "title": "Datefmt"
        },
        "clear": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": false,
          "description": "是否清空日志",
          "title": "Clear"
        }
      },
      "title": "LogConfig",
      "type": "object"
    },
    "Next": {
      "properties": {
        "success": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": "",
          "description": "成功时的下一个任务名",
          "title": "Success"
        },
        "failure": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": "",
          "description": "失败时的下一个任务名",
          "title": "Failure"
        }
      },
      "title": "Next",
      "type": "object"
    },
    "ROI": {
      "properties": {
        "type": {
          "description": "ROI检测后操作类型, 可选: MoveMouse(移动鼠标), DetectOnly(仅检测)",
          "enum": [
            "MoveMouse",
            "DetectOnly"
          ],
          "title": "Type",
          "type": "string"
        },
        "image": {
          "$ref": "#/$defs/Image",
          "description": "ROI图像定义, 包含路径和置信度"
        },
        "window": {
          "anyOf": [
            {
              "$ref": "#/$defs/Window"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "ROI窗口定义, 包含标题, 类名和进程名称, 注意: 某些窗口可能反截屏, 而且可能存在截图兼容性问题"
        },
        "region": {
          "anyOf": [
            {
              "$ref": "#/$defs/Region"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "ROI区域定义, 包含x, y, width, height"
        },
        "duration": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "鼠标移动时间, 单位为毫秒",
          "title": "Duration"
        },
        "debug": {
          "anyOf": [
            {
              "$ref": "#/$defs/ROI_Debug"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "调试参数"
        },
        "returns": {
          "anyOf": [
            {
              "additionalProperties": {
                "enum": [
                  "center_x",
                  "center_y",
                  "confidence",
                  "left",
                  "top",
                  "right",
                  "bottom",
                  "width",
                  "height",
                  "template_height",
                  "template_width"
                ],
                "type": "string"
              },
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "返回值变量字典, 包含['center_x', 'center_y', 'confidence', 'left', 'top', 'right', 'bottom', 'width', 'height', 'template_height', 'template_width'], 以键为变量, 值指定返回参数, 可在其他Job中使用use指定该job返回的参数",
          "title": "Returns"
        }
      },
      "required": [
        "type",
        "image"
      ],
      "title": "ROI",
      "type": "object"
    },
    "ROI_Debug": {
      "properties": {
        "display_screenshot": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "null"
            }
          ],
          "default": false,
          "description": "是否显示截图, 默认为False",
          "title": "Display Screenshot"
        }
      },
      "title": "ROI_Debug",
      "type": "object"
    },
    "Region": {
      "properties": {
        "x": {
          "description": "识别区域的X坐标",
          "title": "X",
          "type": "integer"
        },
        "y": {
          "description": "识别区域的Y坐标",
          "title": "Y",
          "type": "integer"
        },
        "width": {
          "description": "识别区域的宽度",
          "title": "Width",
          "type": "integer"
        },
        "height": {
          "description": "识别区域的高度",
          "title": "Height",
          "type": "integer"
        }
      },
      "required": [
        "x",
        "y",
        "width",
        "height"
      ],
      "title": "Region",
      "type": "object"
    },
    "System": {
      "properties": {
        "type": {
